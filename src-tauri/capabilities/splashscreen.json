{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "splashscreen",
  "description": "enables permissions for splashscreen window",
  "windows": [
    "splashscreen"
  ],
  "local": true,
  "permissions": [
    "core:app:default",
    "core:app:allow-version",
    "http:default",
    "cors-fetch:default",
    "cors-fetch:allow-cors-request",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:4688/*"
        },
        {
          "url": "http://localhost:4689/*"
        }
      ]
    },
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "http://localhost:4688/*"
        },
        {
          "url": "http://localhost:4689/*"
        }
      ]
    }
  ]
}