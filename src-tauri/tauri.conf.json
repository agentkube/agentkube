{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Agentkube",
  "version": "0.1.0",
  "identifier": "platform.agentkube.app",
  "mainBinaryName": "agentkube",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Agentkube",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "hiddenTitle": true
      }
    ],
    "security": {
      "csp": null,

      "capabilities": [
        {
          "identifier": "http:default",
          "description": "Allows making HTTP requests to specific domains",
          "permissions": [{ "identifier": "http:default", "allow": ["http://localhost:4688/**", "http://localhost:65001/**"] }]
        }
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "Agentkube, Inc.",
    "homepage": "https://agentkube.com",
    "copyright": "Â© 2025 Agentkube, Inc. All rights reserved.",
    "license": "MIT",
    "category": "Productivity",
    "shortDescription": "Agentkube: AI-powered Kubernetes management platform.",
    "longDescription": "Agentkube: AI-powered Kubernetes management platform"
  }
}